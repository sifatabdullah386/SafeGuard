<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" android:color="@color/colorTotalList">
        <shape
            android:shape="rectangle">
            <gradient
                android:endColor="#F68A39"
                android:gradientRadius="300dp"
                android:type="radial"
                android:startColor="#FB8E38" />

            <corners android:radius="15dp" />

        </shape>
    </item>
    <item android:color="@color/white">
        <shape
            android:shape="rectangle">
            <gradient
                android:endColor="@color/colorTrafficAccident"
                android:gradientRadius="50dp"
                android:type="radial"
                android:startColor="#EFA165" />

            <corners android:radius="15dp" />

            <stroke
                android:width="4dp"
                android:color="@color/colorTrafficAccident" />
        </shape>
    </item>
</selector>
