<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="grey_font">#ff9e9e9e</color>
    <color name="light_font">#FBFBFB</color>
    <color name="text_shadow">#7F000000</color>
    <color name="text_shadow_white">#FFFFFF</color>
    <string name="app_name">ImageSlider</string>
    <string name="left">LEFT</string>
    <declare-styleable name="ImageSlider">
        <attr format="boolean" name="iss_auto_cycle"/>
        <attr format="integer" name="iss_corner_radius"/>
        <attr format="integer" name="iss_period"/>
        <attr format="integer" name="iss_delay"/>
        <attr format="reference" name="iss_placeholder"/>
        <attr format="reference" name="iss_error_image"/>
        <attr format="reference" name="iss_selected_dot"/>
        <attr format="reference" name="iss_unselected_dot"/>
        <attr format="reference" name="iss_title_background"/>
        <attr format="string" name="iss_text_align"/>
        <attr format="string" name="iss_indicator_align"/>
    </declare-styleable>
</resources>